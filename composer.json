{
  "name": "bugo/smf-faker-generator",
  "type": "project",
  "description": "This package adds fake data for SMF and Light Portal.",
  "keywords": [
    "moonshine admin example",
    "simple machines forum",
    "smf data generator",
    "smf faker data"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "ext-pdo": "*",
    "bugo/moonshine-fontawesome-field": "^2.1",
    "bugo/pexels-laravel": "^0.1",
    "guzzlehttp/guzzle": "^7.10",
    "laravel/framework": "^12",
    "laravel/tinker": "^2.10",
    "moonshine/ace": "^1.0",
    "moonshine/apexcharts": "^1.2",
    "moonshine/easymde": "^2.0",
    "moonshine/moonshine": "^4.0",
    "moonshine/ru": "^1.0",
    "moonshine/tinymce": "^2.0",
    "spatie/laravel-db-snapshots": "^2.7"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.16",
    "beyondcode/laravel-query-detector": "^2.1",
    "bugo/fa-icon-builder": "^0.3",
    "fakerphp/faker": "^1.24",
    "nunomaduro/collision": "^8.8"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "prefer-stable": true
}
